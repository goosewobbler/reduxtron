{
  "extends": ["@electron-toolkit/tsconfig/tsconfig.web.json"],
  "include": [
    "src/shared/**/*",
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.tsx",
    "src/preload/**/*.ts",
    "src/renderer/_preload.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "renderer/*": ["src/renderer/*"],
      "shared/*": ["src/shared/*"]
    }
  }
}
